# Токен Telegram бота (получить у @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# ID чата администратора в Telegram (для получения сообщений от клиентов)
# Получить: отправьте /start боту, затем откройте https://api.telegram.org/bot<TOKEN>/getUpdates
TELEGRAM_ADMIN_CHAT_ID=your_telegram_chat_id_here

# Порт для запуска сервера
PORT=3000

# URL сервера (для виджета на сайте)
# Для тестирования: http://localhost:3000
# Для продакшена: https://your-domain.com
SERVER_URL=http://localhost:3000

